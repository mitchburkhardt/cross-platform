<!DOCTYPE>
<html>
<head>
	<meta name="viewport" content="width=1024, initial-scale=1, user-scalable=no">
	<%
		var style = include(`../slides/${slide}/complete.css`, locals);
		if(locals.nativeRoot){
			style = style.replace(/_#..\/..\/globalAssets/g, '.\/globalAssets').replace(/_#/g, `./${locals.slide}/`);
		}
		else{
			style = style.replace(/_#/g, './').replace(/..\/..\/globalAssets/g, '.\/globalAssets').replace(/.\/.\//g, '.\/').replace(/.\/\//g, '.\/');
		}
	%>
	<%- `<style>${style}</style>` %>


</head>
<body>
<div id="wrapper">
	<div id="container">
		<div class="view"></div>
		<div class="view active">
